<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/unchecked"
        android:drawable="@drawable/vector_ctrl_center_btn_bg_unchecked"
        android:state_checked="false" />

    <item
        android:id="@+id/checked"
        android:drawable="@drawable/vector_ctrl_center_btn_bg_checked"
        android:state_checked="true" />

    <transition
        android:drawable="@drawable/animated_bg_check_500"
        android:fromId="@+id/unchecked"
        android:toId="@+id/checked" />

    <transition
        android:drawable="@drawable/animated_bg_uncheck_170"
        android:fromId="@+id/checked"
        android:toId="@+id/unchecked" />
</animated-selector>