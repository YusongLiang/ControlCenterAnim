<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/unchecked"
        android:drawable="@drawable/ic_gome_ctrl_center_no_disturb_unchecked"
        android:state_checked="false" />

    <item
        android:id="@+id/checked"
        android:drawable="@drawable/ic_gome_ctrl_center_no_disturb_checked"
        android:state_checked="true" />

    <transition
        android:drawable="@drawable/animated_no_disturb_check"
        android:fromId="@+id/unchecked"
        android:toId="@+id/checked" />

    <transition
        android:drawable="@drawable/animated_no_disturb_uncheck"
        android:fromId="@+id/checked"
        android:toId="@+id/unchecked" />
</animated-selector>