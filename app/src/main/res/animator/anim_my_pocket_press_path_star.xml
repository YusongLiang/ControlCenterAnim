<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">
    <objectAnimator
        android:duration="170"
        android:propertyName="strokeColor"
        android:valueFrom="@color/colorPathUnchecked"
        android:valueTo="@color/colorPathChecked"
        android:valueType="colorType" />
    <objectAnimator
        android:duration="170"
        android:propertyName="strokeColor"
        android:startOffset="330"
        android:valueFrom="@color/colorPathChecked"
        android:valueTo="@color/colorPathUnchecked"
        android:valueType="colorType" />
</set>